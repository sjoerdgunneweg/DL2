#!/bin/bash

#SBATCH --partition=gpu_a100
#SBATCH --gpus=1
#SBATCH --job-name=mast3r_eval
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=9
#SBATCH --time=00:59:00
#SBATCH --output=out_files/mast3r_%A.out

module purge
module load 2024
module load Anaconda3/2024.06-1

source activate mast3r
srun python DTUeval-python/eval.py --data "predictions/scan{}_fine_complete_sym_50.ply" --scan_list 24  --mode pcd --dataset_dir data/dtu --vis_out_dir DTUeval-python/vis